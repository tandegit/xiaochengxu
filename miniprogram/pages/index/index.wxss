
.container{ 
  padding: 20rpx;
}
.header{
  background-color: #3d7ef9;
  color: white;
  padding: 15rpx 30rpx;
  width: 85%;
  border-radius: 20rpx;
  box-shadow: #cfcfda 0 0 1rpx;
}
.one{
  display: flex;
  justify-content: space-between;
}                  
.one text{
  font-size: 26rpx;
  height: 30rpx;
  padding-bottom: 15rpx;
}
.two{
  display: flex;
  justify-content: space-between;
}
.two text{
  font-size: 52rpx;
  padding-bottom: 15rpx;
}

.three{
  display: flex;
  justify-content: space-between;
}

.three text{
  font-size: 22rpx;
}
/*body*/
.body-wrapper{
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
}
.date-logo image{
  width: 107rpx;
  height: 107rpx;
}
.date{
  display: flex;
  justify-content: space-around;
  background-color: #fff;
  margin:0rpx 15rpx;
  width: 300rpx;
  height: 150rpx;
  border-radius:40rpx ;
  box-shadow: #b2b2b9 0 0 10rpx;
  padding: 0 10rpx;
}
.date-logo{                                                                                     
  display: flex;
  align-items: center;
}
.date-text{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.date-value{
  font-size: 45rpx;
}

.settemp_css{
  padding: 0%;
}

.settemp_css{
  padding-top: 1rpx;
  display: flex;
  align-items: center;
}

.button{

  display: flex;
  flex-direction: row;
  width:20%;
}